{
  "name": "portal-shell-layout",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/portal/shell/layout/src",
  "prefix": "venusta",
  "tags": ["scope:portal", "type:layout"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/portal/shell/layout/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/portal/shell/layout/**/*.ts",
          "libs/portal/shell/layout/**/*.html"
        ]
      }
    }
  }
}
